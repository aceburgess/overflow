<h1>Log-in to Ov<span style="color: #c0392b;">r</span>flow</h1>
<%= form_tag sessions_path do %>
  <div>
  <%= label_tag :username %>
  <%= text_field_tag :username %>
  </div>
  <div>
  <%= label_tag :password %>
  <%= text_field_tag :password %>
  </div>
  <div class="user_button">
    <%= submit_tag %>
  </div>
<% end %>